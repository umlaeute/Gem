    { $as_echo "$as_me:${as_lineno-$LINENO}: checking target architectures" >&5
$as_echo_n "checking target architectures... " >&6; }

    # Respect TARGET_ARCHS setting from environment if available.
    if test -z "$TARGET_ARCHS"; then
