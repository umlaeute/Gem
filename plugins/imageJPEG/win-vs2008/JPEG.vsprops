<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="JPEG"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(LIBJPEG_DIR)\include"
		PreprocessorDefinitions="HAVE_LIBJPEG"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="jpeg.lib"
		AdditionalLibraryDirectories="$(LIBJPEG_DIR)\lib"
	/>
	<UserMacro
		Name="LIBJPEG_DIR"
		Value="C:\dev\jpeg-6b-4-lib"
	/>
</VisualStudioPropertySheet>
